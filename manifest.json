{
  "manifest_version": 3,
  "name": "LeetCode GTA Mission Sound",
  "version": "1.1",
  "description": "Plays GTA sounds on LeetCode submission result",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["sounds/*.mp3", "injected.js"],
      "matches": ["https://leetcode.com/*"]
    }
  ]
}
